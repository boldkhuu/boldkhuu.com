{
  "name": "boldkhuu.com",
  "description": "Boldkhuu's personal website",
  "version": "0.1.0",
  "author": {
    "name": "Boldkhuu Batbaatar",
    "email": "hi@boldkhuu.com",
    "url": "http://boldkhuu.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boldkhuu/boldkhuu.com"
  },
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "node server.js",
    "lint": "next lint",
    "format": "prettier --write '?(components|pages)/**/*.js'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ],
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.25",
    "@types/react": "^17.0.29",
    "eslint": "7",
    "eslint-config-next": "^11.1.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "pinst": ">=2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "packageManager": "yarn@3.0.2"
}
